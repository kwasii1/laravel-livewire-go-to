{
  "name": "laravel-livewire-go-to",
  "displayName": "laravel-livewire-go-to",
  "description": "Go to support with ctrl+click to switch between components in laravel and livewire",
  "publisher": "kwasibaidoo",
  "version": "1.1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/kwasii1/laravel-livewire-go-to.git"
  },
  "pricing": "Free",
  "engines": {
    "vscode": "^1.102.0"
  },
  "icon": "images/laravel-livewire-go-to-icon.jpg",
  "keywords": [
    "PHP",
    "Laravel",
    "Livewire",
    "Volt",
    "Blade",
    "Flux"
  ],
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:php",
    "onLanguage:blade"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [],
    "languages": [
      {
        "id": "blade",
        "extensions": [
          ".blade.php"
        ],
        "aliases": [
          "Blade",
          "blade"
        ]
      },
      {
        "id": "php",
        "extensions": [
          ".blade.php",
          "php"
        ],
        "aliases": [
          "PHP",
          "php",
          "Php"
        ]
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.102.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "esbuild": "^0.25.3",
    "eslint": "^10.0.1",
    "npm-run-all": "^1.1.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.26.0"
  }
}
